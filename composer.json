{
    "name": "oxygen/auth",
    "description": "Authorisation Component",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "chris@chamberlain.id.au"
        }
    ],
    "require": {
        "php": ">=7.4",
        "oxygen/core": "~0.12.1",
        "oxygen/crud": "~0.9.0",
        "oxygen/data": "~0.11.0",
        "oxygen/preferences": "~0.5.0",
        "illuminate/support": "~6.0",
        "laravel/sanctum": "^2.11",
        "darkghosthunter/laraguard": "dev-master",
        "ext-json": "*",
        "ext-zip": "*",
        "lab404/laravel-impersonate": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Oxygen\\Auth\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Oxygen\\Auth\\AuthServiceProvider",
                "Oxygen\\Auth\\Session\\DoctrineSessionServiceProvider"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "7.4.0"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/oxygen-cms/laraguard-doctrine"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^0.12.18",
        "phpspec/phpspec": "^6.1"
    },
    "scripts": {
        "test": "vendor/bin/phpspec run && vendor/bin/phpstan analyze src --level 1 --memory-limit 256M"
    }
}
